<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="think_plc_inherit_field_project_project" model="ir.ui.view">
        <field name="name">project.project.form.think.plc</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project"/>
        <field name="priority" eval='35'/>
        <field name="arch" type="xml">       
            <field name='user_id' position='before'>
                <field name='is_customize_proj_create' invisible='1'/>
                <field name='x_studio_job_project_type'/>
                <field name='think_job_number'/>
            </field>
            <field name='label_tasks' position='before'>
                <field name='sale_id' invisible="not is_customize_proj_create"/>
                <field name='sale_partner_id' invisible="not is_customize_proj_create"/>
            </field>
        </field>
    </record>
    
</odoo>
